<% if (Array.isArray(opt.stamps)) { %>
  <ul class="stamps">
      <% opt.stamps.forEach(({ src, alt, link }) => { %>
      <% if (link) { %>
        <li>
          <a href="<%= link %>" target="_blank" rel="noopener">
            <img src="<%= src %>" alt="<%= alt %>">
          </a>
        </li>
        <% } else { %>
        <li>
          <img src="<%= src %>" alt="<%= alt %>">
        </li>
      <% } %>
    <% }) %>
<li>
  <meta class="netreviewsWidget" id="netreviewsWidgetNum3835" data-jsurl="//cl.avis-verifies.com/br/cache/8/c/7/8c7c2c69-7520-d754-f544-483d6a0625cb/widget4/widget01-3835_script.js"/><script src="//cl.avis-verifies.com/br/widget4/widget01_FB3.min.js"></script>
   </li>
    <%- await include('@/helpers/widgets-append', {
      _, opt: {}, field: 'stampsAppend'
    }) %>
  </ul>
<% } %>

